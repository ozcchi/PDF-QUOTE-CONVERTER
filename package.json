{
  "name": "pdf-quote-converter",
  "version": "1.0.0",
  "description": "PDF見積書変換ツール",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "concurrently \"webpack serve --mode development\" \"wait-on http://localhost:3000 && tsc -p . && electron .\"",
    "build": "webpack --mode production && tsc -p . && electron-builder",
    "build:webpack": "webpack --mode production",
    "build:ts": "tsc -p .",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\""
  },
  "build": {
    "appId": "com.yourcompany.pdfquoteconverter",
    "productName": "PDF Quote Converter",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "build"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.26.0",
    "@types/electron": "^1.4.38",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.9.1",
    "prettier": "^2.0.0",
    "style-loader": "^3.3.3",
    "typescript": "^5.6.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "lucide-react": "^0.454.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.27.0",
    "tesseract.js": "^5.1.1"
  }
}
